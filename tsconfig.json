{
    "compilerOptions": {
        "target": "ES6",
        "outDir": "./dist/",
        "module": "CommonJS",
        "strict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "esModuleInterop": true,
        "allowJs": true,
        "moduleResolution": "Node",
        "resolveJsonModule": true,
        "lib": [
            "es5",
            "es6",
            "dom",
            "dom.iterable"
        ]
    }
}